<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tea.controllers.MountainLevelController">
    <children>
        <Label layoutX="25.0" layoutY="28.0" text="TEA">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <Label layoutX="33.0" layoutY="105.0" text="Wybierz poziom Tatr:">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <RadioButton layoutX="55.0" layoutY="142.0" mnemonicParsing="false" text="regiel_dolny">
            <toggleGroup>
                <ToggleGroup fx:id="mountainLevel"/>
            </toggleGroup>
        </RadioButton>
        <RadioButton layoutX="55.0" layoutY="173.0" mnemonicParsing="false" text="regiel_górny"
                     toggleGroup="$mountainLevel"/>
        <RadioButton layoutX="55.0" layoutY="204.0" mnemonicParsing="false" text="piętro_kosodrzewiny"
                     toggleGroup="$mountainLevel"/>
        <RadioButton layoutX="55.0" layoutY="235.0" mnemonicParsing="false" text="piętro_hal"
                     toggleGroup="$mountainLevel"/>
        <RadioButton layoutX="55.0" layoutY="266.0" mnemonicParsing="false" text="turnie" toggleGroup="$mountainLevel"/>
        <Button id="next" fx:id="nextButton" layoutX="452.0" layoutY="306.0" mnemonicParsing="false"
                onAction="#handleLevelAction" text="Dalej"/>
        <Label layoutX="324.0" layoutY="373.0" text="Tatra Equipment Advisor, Powered by Karol Kozak, Jakub Piekarz">
            <font>
                <Font size="9.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
